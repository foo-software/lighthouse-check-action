{
  "name": "@foo-software/lighthouse-persist",
  "version": "4.1.0",
  "description": "A tool for persisting Lighthouse audit results used for website performance monitoring and analysis.",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=14.15"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foo-software/lighthouse-persist.git"
  },
  "keywords": [
    "lighthouse",
    "google",
    "performance",
    "seo",
    "progressive web app",
    "best practices",
    "website performance monitoring",
    "AWS",
    "S3"
  ],
  "author": "Adam Henson <adamhenson1979@gmail.com> (https://github.com/adamhenson)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foo-software/lighthouse-persist/issues"
  },
  "homepage": "https://github.com/foo-software/lighthouse-persist#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --single-quote --write",
      "npm run lint"
    ]
  },
  "scripts": {
    "build": "npm run clean && npm run build:js",
    "build:js": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "aws-sdk": "^2.533.0",
    "chrome-launcher": "^0.14.1",
    "lighthouse": "^9.1.0",
    "node-fetch": "^3.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.10",
    "@types/node": "^16.11.10",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.2.5",
    "jest": "^27.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.6.0-dev.20211126"
  }
}
